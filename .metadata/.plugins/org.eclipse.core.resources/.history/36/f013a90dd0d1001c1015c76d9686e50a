package com.java.stream;

public class Product {
  private String name;
  private int id;
  private int price;
  private int quantity;
  
  public Product() {
	super();
	// TODO Auto-generated constructor stub
  }

public Product(String name, int id, int price, int quantity) {
	super();
	this.name = name;
	this.id = id;
	this.price = price;
	this.quantity = quantity;
}
   
  
  
  
}
